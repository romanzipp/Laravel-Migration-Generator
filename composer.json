{
    "name": "romanzipp/laravel-migration-generator",
    "description": "Laravel Migration Generator",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "romanzipp",
            "email": "ich@ich.wtf",
            "homepage": "https://ich.wtf"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/console": "^5.5|^6.0|^7.0|^8.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0",
        "doctrine/dbal": "^2.8",
        "ext-json": "*"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8|^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "friendsofphp/php-cs-fixer": "^2.16"
    },
    "autoload": {
        "psr-4": {
            "romanzipp\\MigrationGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "romanzipp\\MigrationGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "romanzipp\\MigrationGenerator\\Providers\\MigrationGeneratorProvider"
            ]
        }
    }
}
