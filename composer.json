{
    "name": "romanzipp/laravel-migration-generator",
    "description": "Laravel Migration Generator",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "romanzipp",
            "email": "ich@ich.wtf",
            "homepage": "https://ich.wtf"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "doctrine/dbal": "^2.8",
        "illuminate/console": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "mockery/mockery": "^1.3.2",
        "orchestra/testbench": ">=3.8",
        "phpstan/phpstan": "^0.12.99|^1.0",
        "phpunit/phpunit": "^8.0|^9.0",
        "romanzipp/php-cs-fixer-config": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "romanzipp\\MigrationGenerator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "romanzipp\\MigrationGenerator\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "romanzipp\\MigrationGenerator\\Providers\\MigrationGeneratorProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    }
}
